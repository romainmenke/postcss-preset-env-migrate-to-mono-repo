Date: Wed, 28 Jul 2021 01:21:45 +0200
Subject: [PATCH 32/34] update for postcss 8 (postcss/postcss-color-rebeccapurple#16)

* deps: update dependencies

* fix name

* fix urls

* update to postcss 8 api

* deps: update css-values-parser to v5

Co-authored-by: Jonathan Neal <jonathantneal@hotmail.com>
---
 .rollup.js        |   27 +-
 CONTRIBUTING.md   |    2 +-
 README.md         |    4 +-
 package-lock.json | 1109 +++++++++++++++++++++++----------------------
 package.json      |   48 +-
 src/index.js      |   14 +-
 6 files changed, 622 insertions(+), 582 deletions(-)

diff --git a/.rollup.js b/.rollup.js
index 1fb0d87..9ae2db8 100644
--- a/.rollup.js
+++ b/.rollup.js
@@ -1,9 +1,24 @@
-import pkg from './package.json'
+import babel from '@rollup/plugin-babel'
 
 export default {
-	...pkg.rollup,
-	plugins: pkg.rollup.plugins.map(plugin => require(plugin)()),
-	onwarn(warning, warn) {
-		if (warning.code !== 'UNRESOLVED_IMPORT') warn(warning)
-	}
+	input: 'src/index.js',
+	plugins: [
+		babel({
+			babelHelpers: 'bundled',
+			presets: [
+				['@babel/env', {
+					modules: false,
+					targets: { node: 10 }
+				}]
+			]
+		})
+	],
+	external: [
+		'postcss',
+		'postcss-values-parser'
+	],
+	output: [
+		{ file: 'dist/index.cjs.js', format: 'cjs', sourcemap: true, exports: 'default' },
+		{ file: 'dist/index.esm.js', format: 'esm', sourcemap: true, exports: 'default' }
+	]
 }
